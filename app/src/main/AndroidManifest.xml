<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.schedule">

    <application
        android:allowBackup="true"
        android:icon="@drawable/image2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/image2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AddingActivity" android:parentActivityName=".MainActivity" android:launchMode="singleTop"></activity>
        <activity android:name=".WalkThroughActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" android:launchMode="singleTop"/>
    </application>

</manifest>